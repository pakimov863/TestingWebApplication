@addTagHelper *, Microsoft.AspNetCore.Mvc.TagHelpers
@model TestingWebApplication.Models.AdminQuizzes.ImportQuizViewModel

@{
    Layout = "~/Views/Shared/_AdminLayout.cshtml";
    ViewBag.SiteName = "TestingWebApplication";
    ViewBag.Title = "Импорт теста";
}

<form method="post" enctype="multipart/form-data">
    <div asp-validation-summary="All"></div>
    <div class="custom-file">
        <label asp-for="File" class="custom-file-label"></label>
        <input asp-for="File" class="custom-file-input">
    </div>

    <button type="submit">Импортировать</button>
</form>